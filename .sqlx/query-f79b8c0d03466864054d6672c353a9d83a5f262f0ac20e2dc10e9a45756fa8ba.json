  "db_name": "PostgreSQL",
  "query": "\n            SELECT s.* FROM score s\n            JOIN score_rating sr ON s.id = sr.score_id\n            JOIN rating_type rt ON sr.rating_type_id = rt.id\n            WHERE s.user_id = $1 AND rt.name = 'pp'\n            ORDER BY sr.rating_value DESC\n            LIMIT $2\n            ",
